<main>
  <div class="container">
    <div class="row">
      <div class="col-md-8 col-md-offset-2">
        <h4>注文履歴一覧</h4>
        <table class='table'>
          <thead>
            <tr>
              <th>購入日時</th>
              <th>購入者</th>
              <th>注文個数</th>
              <th>注文ステータス</th>
            </tr>
          </thead>
          <tbody>
            <% @orders.each do |order| %>
              <tr>
                <th><%= order.created_at %></th>
                <th><%= order.customer %></th>
                <th><%=</th>
                <th>4</th>
              </tr>
            <% end %>
          </tbody>
        </table>
        <%= paginate @orders %>
    </div>
  </div>
</main>